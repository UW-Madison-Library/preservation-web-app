<%= form_with url: vault_path(vault), method: :get do |form| %>
  <fieldset>
    <%= form.label :preservation_object, 'Enter Preservation Object Identifier' %>
    <%= form.text_field :preservation_object, size: 50, value: params[:preservation_object] %>

    <div class="actions actions--small-margin">
      <%= form.submit 'Submit' %>
    </div>
  </fieldset>
<% end %>
